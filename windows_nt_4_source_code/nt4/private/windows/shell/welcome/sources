MAJORCOMP=windows
MINORCOMP=shell

TARGETNAME=welcome
TARGETPATH=obj
TARGETTYPE=LIBRARY
TARGETLIBS=

INCLUDES=.;..\inc;..\..\inc

C_DEFINES= -DWIN32 -DWINNT -DUNICODE

SOURCES=welcome.c  \
	welcome.rc

UMTYPE=windows
UMAPPL=welcome
!IF $(386)
UMENTRYABS=ModuleEntry@0
!ELSE
UMENTRYABS=ModuleEntry
!ENDIF

UMLIBS=$(BASEDIR)\public\sdk\lib\*\kernel32.lib  \
       $(BASEDIR)\public\sdk\lib\*\shell32.lib   \
       $(BASEDIR)\public\sdk\lib\*\gdi32.lib     \
       $(BASEDIR)\public\sdk\lib\*\user32.lib    \
       $(BASEDIR)\public\sdk\lib\*\advapi32.lib  \
       $(BASEDIR)\public\sdk\lib\*\shell32.lib   \
       obj\*\welcome.lib                         \
       obj\*\welcome.res
