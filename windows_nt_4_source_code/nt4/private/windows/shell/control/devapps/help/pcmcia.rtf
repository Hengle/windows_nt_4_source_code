\par \page {\cs15\fs16\up6 #{\footnote \pard\plain \s16\li115\sb120\sl240\slmult0\widctlpar \f2\fs20 {\cs15\fs16\up6 #} PCMCIA_DEVICES_SHOW_CONTROL_ON_TASKBAR}}
