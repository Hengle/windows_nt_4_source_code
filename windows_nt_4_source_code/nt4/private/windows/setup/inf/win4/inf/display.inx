;
; Matching section.  Provides some mappings for some names that don't
; show up in the above manufacturer \ models list.
; This is basically to handle backwards compatibility with some drivers
; or boards we can't list for some reason.
;

[Models Matching]





;
; List of drivers that are provided, which each model has a reference to
;
;
; ati
; cirrus
; dell_dgx
; et4000
; jazzg300
; jazzg364
; jzvxl484
; mga
; mga_mil
; ncr77c22
; psidisp
; qv
; s3
; tga
; v7vram
; wd90c24a
; wdvga
; weitekp9
; xga
;
; The below list are not autodetected
;
; 8514a
; vga
; trident
;




; detection section


[detect]
CopyFiles=detect.Miniport, detect.Display

[detect.Services]
@a:ati         = ATI_COMPAT
@i:ati         = ATI_COMPAT
@p:ati         = ATI_COMPAT
@@:cirrus      = CIRRUS_COMPAT
@i:dell_dgx    = DELL_DGX_COMPAT
@i:et4000      = ET4000_COMPAT
@m:jazzg300    = JAZZG300_COMPAT
@m:jazzg364    = JAZZG364_COMPAT
@m:jzvxl484    = JZVXL484_COMPAT
@a:mga         = MGA_COMPAT
@i:mga         = MGA_COMPAT
@@:mga_mil     = MGA_MIL_COMPAT
@i:ncr77c22    = NCR77C22_COMPAT
@p:psidisp     = PSIDISP_COMPAT
@a:qv          = QV_COMPAT
@i:qv          = QV_COMPAT
@@:s3          = S3_COMPAT
@a:tga         = TGA_COMPAT
@m:tga         = TGA_COMPAT
@i:v7vram      = V7VRAM_COMPAT
@p:wd90c24a    = WD90C24A_COMPAT
@i:wdvga       = WDVGA_COMPAT
@@:weitekp9    = WEITEKP9_COMPAT
@i:xga         = XGA_COMPAT

; 8514a       don't detect since we generally want to use ATI
; trident     we don't detect trident because old driver crashes on new chips.
; vga         we never need to detect vga because of vga save.



; 8514a Driver Section


[8514a]
CopyFiles=8514a.Miniport, 8514a.Display

; Software Installation

[8514a.SoftwareSettings]
AddReg = 8514a_SoftwareDeviceSettings

; Service Installation

[8514a.Services]
AddService = 8514a, 0x00000002, 8514a_Generic_Service_Inst, 8514a_EventLog_Inst

[8514a_EventLog_Inst]
AddReg = 8514a_EventLog_AddReg

[8514a_EventLog_AddReg]
HKR,,EventMessageFile,0x00020000,"%SystemRoot%\System32\IoLogMsg.dll;%SystemRoot%\System32\drivers\8514a.sys"
HKR,,TypesSupported,0x00010001,7

[8514a_Generic_Service_Inst]
ServiceType    = 1                  ; SERVICE_KERNEL_DRIVER
StartType      = 1                  ; SERVICE_SYSTEM_START
ErrorControl   = 0                  ; SERVICE_ERROR_IGNORE
LoadOrderGroup = Video
ServiceBinary  = %12%\8514a.sys



