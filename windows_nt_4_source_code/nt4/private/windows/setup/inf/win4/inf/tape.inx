[qic117_EventLog_AddReg]
HKR,,EventMessageFile,%REG_EXPAND_SZ%,"%%SystemRoot%%\System32\IoLogMsg.dll;%%SystemRoot%%\System32\drivers\qic117.sys"
HKR,,TypesSupported,%REG_DWORD%,7

[qic157_Inst]
