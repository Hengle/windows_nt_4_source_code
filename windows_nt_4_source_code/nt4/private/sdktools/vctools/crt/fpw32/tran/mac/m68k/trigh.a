#ifndef SANE

#include <traps.a>

	externW _errno


;double sinh(double x)

cProc sinh,PUBLIC
	parmQ x
cBegin sinh
      	fmove.l fpcr,d0
      	fmove.l #0,fpcr
      	fsinh.d x,fp0
	fmove.l fpsr, d1        ;get status word
	and.l #0x03007c00, d1  ;see if we get an exception
	cmp.l #0, d1
	bne range_error_fpu1
	jra to_end_fpu1

range_error_fpu1:
	move.l #34, _errno
to_end_fpu1:	
      	fmove.l	d0,fpcr
cEnd sinh


