$(OBJDIR)\trnsctrl.obj: 
!IF "$(KIND)" == "SWAPF"  
	$(CPP) -DSWAPPER $(CPPFLAGS) $(CPPFILES)
	$(AAS) $(AAFLAGS:% -S -T -D=EH) $(AAFILES)
!ELSE
!IF "$(KIND)" == "NOSWAPF"
	$(CPP) $(CPPFLAGS) $(CPPFILES)
	$(AAS) $(AAFLAGS:%  -T -D=EH) $(AAFILES)
!ELSE
!IF "$(KIND)" == "SWAP"
	$(CPP) -DSWAPPER $(CPPFLAGS) $(CPPFILES)
	$(AAS) $(AAFLAGS:% -S=EH) $(AAFILES)
!ELSE
	$(CPP) $(CPPFLAGS) $(CPPFILES)
	$(AAS) $(AAFLAGS:%=EH) $(AAFILES)
!ENDIF
!ENDIF
!ENDIF
#	-del $(AAFILES) 2>nul


