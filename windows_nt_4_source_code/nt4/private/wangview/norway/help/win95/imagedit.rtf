\par }\pard\plain \s31\li115\sb160 \b\f7\fs18 Arguments
\par \trowd \trgaph108\trleft283 \cellx2400\cellx3720\cellx4586\cellx7679 \pard\plain \s33\sb120\sl-240\slmult1\intbl\brdrb\brdrs\brdrw15\brsp20 \tx900\tx1062\tx1242\tx1422\tx1602\tx1782\tx1980\tx2368 \b\f7\fs16 Parameter\cell \pard 
