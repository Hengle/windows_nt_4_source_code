\par \trowd \trgaph108\trleft284 \cellx1920\cellx3360\cellx6804 \pard\plain \s33\sb120\sl-240\slmult1\intbl\brdrb\brdrs\brdrw15\brsp20 \tx900\tx1062\tx1242\tx1422\tx1602\tx1782\tx1980\tx2368 \b\f7\fs16 Parameter\cell \pard \s33\sb120\sl-240\slmult1\intbl\brdrb
