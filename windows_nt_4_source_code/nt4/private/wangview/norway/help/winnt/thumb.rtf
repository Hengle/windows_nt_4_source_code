\par \trowd \trgaph108\trleft270 \cellx1843\cellx3060\cellx6480 \pard\plain \s54\sb120\sl-240\slmult1\intbl\brdrb\brdrs\brdrw15\brsp20 \tx900\tx1062\tx1242\tx1422\tx1602\tx1782\tx1980\tx5070 \b\f7\fs16 Parameter\cell Data Type\cell Description\cell 
