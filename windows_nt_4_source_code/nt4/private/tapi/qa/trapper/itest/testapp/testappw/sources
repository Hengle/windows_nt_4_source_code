           $(BASEDIR)\public\sdk\lib\unicode\*\tcore.lib \
           $(BASEDIR)\public\sdk\lib\unicode\*\ttest.lib \
           $(BASEDIR)\public\sdk\lib\*\tapi32.lib


C_DEFINES=$(C_DEFINES) -DWUNICODE=1
