FONT 8, "MS Sans Serif"
BEGIN
    CONTROL         "",IDC_TREEWND,"SysTreeView32",TVS_HASBUTTONS | TVS_HASLINES | 
                    WS_BORDER | WS_TABSTOP | TVS_DISABLEDRAGDROP ,7,7,125,216,
                    WS_EX_CLIENTEDGE
    EDITTEXT        IDC_EDITWND,139,7,274,216,ES_AUTOHSCROLL | ES_READONLY | 
                    ES_WANTRETURN
END



