                    56, 155, 75, 12
    PUSHBUTTON      "&Add >", IDC_TC_PB_ADD, 38, 175, 40, 14
    PUSHBUTTON      "< &Remove", IDC_TC_PB_DELETE, 82, 175, 40, 14
    LTEXT           "E&xcluded ranges", IDC_TC_ST_EXCL_RANGES, 138, 125, 64, 
                    8
