
NO_CAIRO_LIBS=1

MAJORCOMP=iballs
MINORCOMP=proxy
TARGETNAME=iballs

DLLDEF=obj\*\iballs.def

#
# Add this to the coffbase.txt file
#
DLLBASE=0x1b500000

#
# We don't want the .lib file in sdk\lib.  Binplace.exe will put the DLL in
# the release tree.
#
TARGETPATH=obj
TARGETTYPE=DYNLINK

TARGETLIBS= \
    $(BASEDIR)\public\sdk\lib\*\gdi32p.lib    \
    $(BASEDIR)\public\sdk\lib\*\ntdll.lib     \
    $(BASEDIR)\public\sdk\lib\*\ole32.lib     \
    $(BASEDIR)\public\sdk\lib\*\kernel32.lib  \
    $(BASEDIR)\public\sdk\lib\*\RpcRt4.lib


LINKLIBS= \
         ..\..\uuid\daytona\obj\*\uuid.lib \
         $(BASEDIR)\public\sdk\lib\*\uuid.lib

INCLUDES=..\..\..\..\ole32\ih
INCLUDES=$(INCLUDES);$(BASEDIR)\private\cinc

CONDITIONAL_INCLUDES=


SYNCHRONIZE_BLOCK=1
SYNCHRONIZE_DRAIN=1

!include       ..\..\..\daytona.inc

C_DEFINES = $(C_DEFINES)

LINKER_FLAGS = /MERGE:.text=.orpc

# All the files listed below ending in _p.c are generated by MIDL

SOURCES= \
         ..\transmit.cxx \
         ..\dlldata.c    \
         ..\iballs_p.c   \
         ..\icube_p.c    \
         ..\ibtest_p.c   \
         ..\iloop_p.c    \
         ..\rpctst_p.c

