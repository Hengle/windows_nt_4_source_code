MIDL_FLAGS=-Oi -oldnames -I\nt\public\sdk\inc

all:            dll\goober.h

dll\goober.h:   goober.idl
                midl $(MIDL_FLAGS) \
                     -proxy dll/goober_p.c  \
                     -dlldata dll/dlldata.c \
                     -header dll/goober.h   \
                     -iid common/goober_i.c  \
                     goober.idl

clean:          sweep all

sweep:
                del dll\goober.h

