MAJORCOMP=utils
MINORCOMP=findstr

USE_CRTDLL=1
C_DEFINES = $(C_DEFINES) -DDBCS=1

TARGETNAME=findstr
TARGETPATH=obj
TARGETTYPE=PROGRAM

SOURCES=qmatch.c   \
        recurse.c  \
        findstr.c  \
        findstr.rc \
        fsmsg.mc

UMTYPE=console

PASS0_HEADERDIR=.
PASS0_SOURCEDIR=.
MC_FLAGS=-v

USECXX_FLAG=/Tp

TARGETLIBS= \
    $(BASEDIR)\public\sdk\lib\*\binmode.obj \
    $(BASEDIR)\public\sdk\lib\*\user32.lib
