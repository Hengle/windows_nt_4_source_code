TARGETNAME=hours
TARGETPATH=obj
TARGETTYPE=DYNLINK
TARGETEXT=ocx
DLLBASE=0x5a500000
DLLENTRY=_DllMainCRTStartup

USE_MFCUNICODE=1

PRECOMPILED_INCLUDE=stdafx.h
PRECOMPILED_CXX=1

SOURCES= \
	hours.cpp       \
	hoursctl.cpp    \
	hoursppg.cpp    \
	hours.rc

UMTYPE=windows

TARGETLIBS= \
    $(BASEDIR)\public\sdk\lib\*\gdi32.lib \
    $(BASEDIR)\public\sdk\lib\*\user32.lib \
    $(BASEDIR)\public\sdk\lib\*\oleaut32.lib \
    $(BASEDIR)\public\sdk\lib\*\kernel32.lib

